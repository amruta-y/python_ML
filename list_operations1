#accessing each element of a list
list1= ['Physics','Chemistry','Maths',10,20,30]
list2=[1,2,3,4,5]
print("list1: ",list1[0:3])
print("list1: ",list2[2:3])

#add two lists
list1= ['Physics','Chemistry','Maths',10,20,30]
list2=[1,2,3,4,5]
list3=list1+list2
print(list3)

#scaling the list
list1= ['Physics','Chemistry','Maths',10,20,30]
list2=[1,2,3,4,5]
list3=list2*3
print(list3)

