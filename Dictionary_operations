thisdict = {
  "brand": "Ford",
  "model": "Mustang",
  "year": 1964
}
print(thisdict["brand"])


#length of the dict()
print(len(thisdict))


#pop
thisdict.pop("year")

#update
dict1={
  "brand": "Audi",
  "model": "s class",
  "year": 1968
}
thisdict.update(dict1)
print(thisdict)

#getting key
a=thisdict.keys()
print(a)


#getting values
thisdict.values()
